            var cuserrormess = "O nome de usu&aacute;rio ou a senha inserida est&aacute; incorreta. Tente novamente. Se voc&ecirc; n&atilde;o conseguir fazer logon, entre em contato com o <a href='https://atendimento.univesp.br/sae/portal.html' target='_blank' style='text-decoration: underline'>SAE - Sistema de Atendimento Eletr&ocirc;nico</a>.";
            function loadLoginPage() {
                if(document.forms.login.user_id != undefined) {
                    verify_username_password(document.forms.login);
                }
                setTimeout("triggerScreenreaderAlert()", 500);
            }

            function triggerScreenreaderAlert() {
                if (document.getElementById('loginErrorMessage')) {
                    //$('loginErrorMessage').update($('loginErrorMessage').innerHTML);
                    $('loginErrorMessage').update(cuserrormess);
                }
            }
            var cimg = document.createElement('img');
            cimg.src = 'https://ava.univesp.br/bbcswebdav/xid-130628_1';
            cimg.className = "cpasswordeye";
            document.getElementById('password').parentElement.appendChild(cimg);

            var eyebutton = document.getElementsByClassName('cpasswordeye');
            if(eyebutton[0]) {
                eyebutton[0].onclick = function() {
                    if (document.getElementById('password').type == 'password') {
                        document.getElementById('password').type = 'text';
                        eyebutton[0].addClassName('closecpasswordeye');
                    } else {
                        document.getElementById('password').type = 'password';
                        eyebutton[0].removeClassName('closecpasswordeye');
                    }
                };
            }

            window.onload = function() {
                replace_name();
            };

            function replace_name() {
                var replacethis = document.querySelector('#login-form label[for="user_id"]');
                var tochange = "E-mail";
                if (replacethis) {
                    replacethis.innerText = tochange;
                }
            }
            // Add Fonts
            var cushead = document.head;
            var cuslink = document.createElement("link");
            cuslink.rel = "stylesheet";
            cuslink.href = "https://fonts.googleapis.com/css2?family=Barlow:wght@600&display=swap";
            cushead.appendChild(cuslink);

            // Boxes and form/icons.
            var buttono365 = document.getElementsByClassName('logininstruction o365licono');
            var buttonnormal = document.getElementsByClassName('logininstruction lnormal');
            var buttonava = document.getElementsByClassName('custavas');
            if (buttono365[0] && buttonnormal[0]) {
                buttono365[0].onclick = function() {
                    document.getElementsByClassName('loadersniper')[0].setStyle("display: block;");
                    buttonnormal[0].setOpacity(0.4);
                    buttonava[0].setOpacity(0.4);
                    buttono365[0].setOpacity(1);
                    document.querySelector("#login-form").setStyle("display: none;");
                    document.querySelector(".loginiconscontainer").setStyle("display: block;");
                    document.querySelector("#loginRedirectProviderList").setStyle("display: none;");
                    if (document.querySelector("#loginRedirectProviderList").children[1]) {
                        document.querySelector("#loginRedirectProviderList").children[1].childElements()[0].click();
                    }
                };
                buttonnormal[0].onclick = function() {
                    buttono365[0].setOpacity(0.4);
                    buttonava[0].setOpacity(0.4);
                    buttonnormal[0].setOpacity(1);
                    document.querySelector("#login-form").setStyle("display: block;");
                    document.querySelector("#login-form form").setStyle("display: block;");
                    document.querySelector("#login-form #loginRedirectProviders").removeClassName("showtercero");
                    document.querySelector("#login-form #loginRedirectProviders").setStyle("display: none;");
                    document.querySelector(".loginiconscontainer").setStyle("display: none;");
                    document.querySelector("#loginRedirectProviderList").setStyle("display: none;");
                    document.getElementsByClassName('loadersniper')[0].setStyle("display: none;");
                };
            }
            
            if (buttonava[0]) {
                buttonava[0].onclick = function(e) {
                    e.preventDefault();
                    document.getElementsByClassName('loadersniper')[0].setStyle("display: block;");
                    buttono365[0].setOpacity(0.4);
                    buttonnormal[0].setOpacity(0.4);
                    buttonava[0].setOpacity(1);
