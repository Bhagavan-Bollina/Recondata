  <script type="text/javascript">

    jQuery(document).ready(function() {
	    jQuery('#fullpage').fullpage({
	    	responsiveWidth:992,
	        scrollBar: true
	    });
	    escala();
	    jQuery('link[rel=stylesheet][id~="css_0"]').remove();
	    jQuery('link[rel=stylesheet][id~="css_1"]').remove();
	    jQuery('link[rel=stylesheet][id~="css_2"]').remove();
	    jQuery('#entry-login').addClass('btn.btn-warning');
	    jQuery('input[type=text]').addClass('form-control');
	    jQuery('input[type=password]').addClass('form-control');
	    jQuery('font').removeAttr('style');
	    jQuery('font').removeAttr('face');
	    jQuery('font').removeAttr('size');
	
		});

    function irFAQ(){
        jQuery.fn.fullpage.moveTo(2);
    }
      function loadLoginPage()
      {
    	  if ( top != self )
    	  {
    		  top.location.replace( self.location.href );
    		}
    	  if(document.forms.login.user_id != undefined)
    		{
    		  document.forms.login.user_id.focus();
    		}
    	  setTimeout("triggerScreenreaderAlert()", 500);
      }
      
      function triggerScreenreaderAlert()
      {
    	if ( document.getElementById( 'loginErrorMessage' ) )
  	    {
    	  $( 'loginErrorMessage' ).update( $('loginErrorMessage').innerHTML );
  	    }
      }
     
    </script>
    <script type="text/javascript">
    	
    	 function escala() {
    		var pantallaCompleta = jQuery(window).height();
    		if (jQuery(window).width() > 768) {
	        jQuery('[data-resize]').each(function(index) {
	            medida = Math.floor((jQuery(this).attr("data-resize") * pantallaCompleta) / 100) - .3;
	            jQuery(this).css("height", medida);
	        });
	        //console.log("ponlo fixed");
	        jQuery('footer').css({
	            "position": "fixed"
	        })
	        jQuery('#closeFooterButton').hide();

    		} else {
	        jQuery('footer').css({
	            "position": "relative"
        	})
    		}

	    //console.log("conviertiendo menu");
	    jQuery('.levelHolderClass ul').css({
	        "height": pantallaCompleta - 100
	    });
	
	    jQuery('[data-resizem]').each(function(index) {
	
	        medida = Math.floor((jQuery(this).attr("data-resizem") * pantallaCompleta) / 100) - .3;
	        jQuery(this).css("height", medida);
	
	    });
	    ;
		}
	</script>
  
 
