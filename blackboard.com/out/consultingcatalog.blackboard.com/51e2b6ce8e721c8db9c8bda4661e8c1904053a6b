    window.getRegionData = function () {
        var data = {
            '': 'Please Select a Region',
            'NA': 'North America (NA) ',
            'RMEA': 'Russia, Middle East and Africa (RMEA) ',
            'EU': 'Europe (EU)',
            'APAC': 'Asia Pacific (APAC)',
            'LAC': 'Latin America and the Caribbean (LAC) '
        };
        var s = $('<select />');
        for (var val in data) {
            $('<option />', {
                value: val,
                text: data[val]
            }).appendTo(s);
        }
        s.addClass("ui-txt-general medium");
        return s;
    }

