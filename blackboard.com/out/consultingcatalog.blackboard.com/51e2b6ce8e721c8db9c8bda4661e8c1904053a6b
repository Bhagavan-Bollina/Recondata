<script type="text/javascript">
    var isMyViewPage = ($("#pw-body").length) ? true : false;
    var csScrollBtnIsSticky = false, csScrollBtnIsVisible = false, csScrollBtnReset = false;
	var csFooterAccordionIsSet = false;
    var csPageListAccordionIsSet = false;
    var csSlideshowDescFixed = false;
    var csViewportSize = $(window).width();
    var csKeyCodes = { "tab": 9, "enter": 13, "esc": 27, "space": 32, "end": 35, "home": 36, "left": 37, "up": 38, "right": 39, "down": 40 };
	
    $(window).load(function(){
    	calendarListView();
        makeNavAppAccessible();
    });

    $(function(){
    	$("body").attr("data-cs-breakpoint", csGetBreakPoint());
        $(window).resize(function(){ $("body").attr("data-cs-breakpoint", csGetBreakPoint()); jsMediaQueries(); calendarListView(); checkSlideshow(); adjustCardAppTitles(); });
        $(window).scroll(function() { checkSlideshow(); });
		
        csTranslate();
        channelBarEvents();
        solutionsDropdownEvents();
        jsMediaQueries();
        useChannelForSection();
        dynamicContent();
