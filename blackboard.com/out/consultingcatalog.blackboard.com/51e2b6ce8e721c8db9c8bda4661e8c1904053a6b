/* GroupBegin Icons */
@font-face {
    font-family: 'ceHub';
    src: url("data:application/x-font-ttf;charset=utf-8;base64,") format('truetype');
    font-weight: normal;
    font-style: normal;
}
[class^="icon-"], [class*=" icon-"] {
    font-family: 'ceHub' !important;
    speak: none;
    font-style: normal;
    font-weight: normal;
