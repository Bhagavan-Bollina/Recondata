    window.getTitleData = function () {
        var data = {
            '': 'Please Select a Title',
            'Administrator/Assistant/Coordinator': 'Administrator/Assistant/Coordinator',
            'Associate Director': 'Associate Director',
            'Associate Vice President/Associate Provost': 'Associate Vice President/Associate Provost',
            'Consultant': 'Consultant',
            'Dean': 'Dean',
            'Director': 'Director',
            'Executive (President, Chancellor, Provost, CEO, CIO, CAO, CTO, COO)': 'Executive (President, Chancellor, Provost, CEO, CIO, CAO, CTO, COO)',
            'Manager': 'Manager',
            'Professor/Instructor': 'Professor/Instructor',
            'Student': 'Student',
            'Technician/Specialist': 'Technician/Specialist',
            'Vice President/Vice Provost/Vice Chancellor/Executive Vice President': 'Vice President/Vice Provost/Vice Chancellor/Executive Vice President'
        };
        var t = $('<select />');
        for (var val in data) {
            $('<option />', {
                value: val,
                text: data[val]
            }).appendTo(t);
        }
        t.addClass("ui-txt-general medium");
        return t;
    }
    
    window.registerAccount = function () {
            var s = getRegionData();
            var c = getCountryData();
            var t = getTitleData();

            //Phone Override
            $("label[for='swregisteruser-txt-phone']").text('Business Phone:');

            //Username Override
            $("#swregisteruser-txt-username").prop("readonly", true);
            $("#swregisteruser-txt-username").addClass('bbgs-input-disabled');

            //Email Override
            $("#swregisteruser-txt-emailaddress").keyup(function () {
                $("#swregisteruser-txt-username").val($("#swregisteruser-txt-emailaddress").val());
            });

            //Region Override
            $("label[for='swregisteruser-txt-zipcode']").text('Region:');
            var region = $('#swregisteruser-txt-zipcode').val();
            $('#swregisteruser-txt-zipcode').remove();
            s.attr("name", "swregisteruser-txt-zipcode");
            s.attr("id", "swregisteruser-txt-zipcode");
            s.insertAfter($("label[for='swregisteruser-txt-zipcode']"));

            //Country Override
            $("label[for='swregisteruser-txt-address1']").html("<span class='recStar' aria-label='required item'>*</span>Country:");
            var country = $('#swregisteruser-txt-address1').val();
            $('#swregisteruser-txt-address1').remove();
            c.attr("name", "swregisteruser-txt-address1");
            c.attr("id", "swregisteruser-txt-address1");
            c.addClass("required");
            c.attr("validationtype", "text");
            c.attr("errormessage", "Country is required.");
            c.insertAfter($("label[for='swregisteruser-txt-address1']"));

            //Title Override
            $("label[for='swregisteruser-txt-title']").text('Title:');
            var title = $('#swregisteruser-txt-title').val();
            $('#swregisteruser-txt-title').remove();
            t.attr("name", "swregisteruser-txt-title");
            t.attr("id", "swregisteruser-txt-title");
            t.insertAfter($("label[for='swregisteruser-txt-title']"));

            //Organization Override
            $("label[for='swregisteruser-txt-address2']").text('Organization:');

            //Hide all others
            $('#swregisteruser-txt-city').hide();
            $("label[for='swregisteruser-txt-city']").hide();
            $('#swregisteruser-txt-state').hide();
            $("label[for='swregisteruser-txt-state']").hide();
            $('#swregisteruser-txt-fax').hide();
            $("label[for='swregisteruser-txt-fax']").hide();

            //Terms of Use and Privacy
            $("<font size=\"2\">Your privacy is important to us. We will only use this information for the purpose of providing this catalogue and the related services to you. We may share your information with the relevant local Blackboard channel partners (resellers) if your organization is located in an area managed by Blackboard partners <a href=\"http://www.blackboard.com/about-us/locations/find-a-channel-partner.html\" target=\"_blank\" title=\"Find a Channel Partner\">(see list here)</a>. View our <a href=\"http://www.blackboard.com/footer/privacy-policy.html\" target=\"_blank\" title=\"Blackboard Privacy Policy\">Privacy Statement</a> for more details.</font>").insertAfter($("[radioname$='swregisteruser-chk-termsofuse']"))
            $("[radioname$='swregisteruser-chk-termsofuse']").hide();
            $("#swregisteruser-chk-termsofuse")[0].checked = true;
        };

    window.showMyAccount = function () {
        var s = getRegionData();
        var c = getCountryData();
        var t = getTitleData();

        //Username Override
        $("#myaccount-txt-username").prop("readonly", true);
        $("#myaccount-txt-username").addClass('bbgs-input-disabled');

        //Email Override
        $("#myaccount-txt-email").prop("readonly", true);
        $("#myaccount-txt-email").addClass('bbgs-input-disabled');

        //Phone Override
        $("label[for='myaccount-txt-phone']").text('Business Phone:');

        // Region Override
        $("label[for='myaccount-txt-zip']").text('Region:');
        $("label[for='myaccount-txt-zip']").prop("id", "custom-zip");
        var region = $('#myaccount-txt-zip').val();
        $('#myaccount-txt-zip').remove();
        s.attr("name", "myaccount-txt-zip");
        s.attr("id", "myaccount-txt-zip");
        s.insertAfter($("label[for='myaccount-txt-zip']"));
        s.val(region);

        // Country Override
        $("label[for='myaccount-txt-street1']").text('Country:');
        $("label[for='myaccount-txt-street1']").prop("id", "custom-country");
        var country = $('#myaccount-txt-street1').val();
        $('#myaccount-txt-street1').remove();
        c.attr("name", "myaccount-txt-street1");
        c.attr("id", "myaccount-txt-street1");
        c.insertAfter($("label[for='myaccount-txt-street1']"));
        c.val(country);

        // Title Override
        $("label[for='myaccount-txt-title']").text('Title:');
        $("label[for='myaccount-txt-title']").prop("id", "custom-title");
        var title = $('#myaccount-txt-title').val();
        $('#myaccount-title-label').remove();
        $('#myaccount-txt-title').remove();
        t.attr("name", "myaccount-txt-title");
        t.attr("id", "myaccount-txt-title");
        t.insertAfter($("label[for='myaccount-txt-title']"));
        t.val(title);

        // Organization Override
        $("<label id=\"custom-organization\" for=\"myaccount-txt-street2\">Organization:</label>").insertBefore($('#myaccount-txt-street2'));

        //Hide all others
        $("#myaccount-txt-city").hide();
        $("label[for='myaccount-txt-city']").hide();
        $("#myaccount-txt-state").hide();
        $("label[for='myaccount-txt-state']").hide();

    };

    $(document).ready(function () {

        document.cookie = "UnderAge=false;  path=/; max-age=31536000";

        if ($("#hid-pageid").val() == "0") {
            $(".sp-breadcrumbs").hide();
        }

        $('.ui-widget-header .ui-description').text('Enter your details below to complete the Consulting Catalog User Registration process.  Fields denoted with an asterisk (*) are required.');

        $('#myaccount-connections').hide();
        $('#myaccount-pnl-content').css({
            "width": "86%",
            "margin": "-10px 0 0px 0px"
        });
        $('#myaccount-pnl-nav').css("width", "14%");
        $('#myaccount-btn-subscriptions').removeAttr("onclick");
        $('#myaccount-btn-subscriptions').off('click');
        $('#myaccount-btn-subscriptions').click(function () {
            $("#myaccount-pnl-nav > .ui-table > .selected").removeClass("selected");
            $("#myaccount-pnl-nav > .ui-table > #myaccount-subscriptions").addClass("selected");
            $('#myaccount-pnl-content').empty();
            $('#myaccount-pnl-content').append("<div id='cnslt-subscription' class='ui-widget nested'>");
            $("#cnslt-subscription").html("<iframe src='/cms/Module/LTI/ltiLaunch.aspx?ModuleInstanceId=258' id='cnslt-lti'></iframe'");
        });
        // allow our custom page to load when tabs are clicked
        $('#myaccount-btn-information').removeAttr("onclick");
        $('#myaccount-btn-information').off('click');
        $('#myaccount-btn-information').click(function () {
            var success =
                '$(\'#myaccount-pnl-nav div\').removeClass(\'selected\');$(\'#myaccount-general\').addClass(\'selected\');showMyAccount();';
            var loadPath = $('#myaccount-hid-fullpath').val() +
                '/cms/UserControls/MyAccount/MyAccountGeneralEditWrapper.aspx';
            // If secure site, send securely
            if ($('#myaccount-hid-issecure').val() == 'True') {
                loadPath = loadPath.replace('http://', 'https://');
            }
            GetContent(loadPath, 'myaccount-pnl-content', 2, success);
        });

        function getParameterByName(name) {
            var match = RegExp('[?&]' + name + '=([^&]*)').exec(window.location.search);
            return match && decodeURIComponent(match[1].replace(/\+/g, ' '));
        }
        var tab = getParameterByName('tab');
        if (tab && tab.toLowerCase() == 'subscriptions') {
            $('#myaccount-btn-subscriptions').click();
        }
        //Register Page
        if ($('#sw-pnl-registeruser').length) {
            registerAccount();
        }

        if ($('#myaccountgeneral-pnl-detail').length) {
            showMyAccount();
        }

    });
