    function jsMediaQueries(){
        switch(csGetBreakPoint()) {
            case "desktop": 
            	footerAccordionReset();
            break;
            case "768":
            	footerAccordionReset();
            break;
            case "640":
            	footerAccordion();
            break;
            case "480":
            	footerAccordion();
            break;
            case "320":
            	footerAccordion();
            break;
        }
    }
    
    function rsMenu() {
    	// RESPONSIVE MAIN MENU
        $.csRsMenu({
            "breakPoint" : 768,
            "slideDirection" : "right-to-left",
            "menuButtonParent" : "#gb-universal-navigation",
            "menuBtnText" : "Menu",
            "showTranslate" : true,
            "translateMenuText": "Translate/Traducir",
			"translateVersion": 2,
            "showAccount": true,
            "accountMenuText": "Account",
            "siteID": "4",
            "allLoaded": function(){}
        });
        
    }
	
    function mediaRotator() {
        if(!$("#sw-content-container10.region.ui-hp .rs-gallery-container").length){
        	var transitionSpeed = 2;
        
            // MULTIMEDIA ROTATOR
            $("#sw-content-container10.ui-hp div.ui-widget.app.cs-rs-multimedia-rotator").csRsMultimediaRotator({
                "imageWidth"              : 1220,
                "imageHeight"             : 462,
                "efficientLoad"           : "yes",
                "appHeader"               : "no",
                "titles"                  : "yes",
                "captions"                : "yes",
                "galleryOverlay"          : "no",
                "linkedElement"           : ["image"], // ["image", "title", "overlay", "all"]
                "playPause"               : "no",
                "mainControls"            : "no",
                "contMoreLink"            : "no",
                "contMoreLinkText"        : "Read More",
                "descMoreLink"            : "yes",
                "descMoreLinkText"        : "Learn More",
                "descVideoLink"			  : "yes",
                "descVideoLinkText"		  : "Play Video",
                "bullets"                 : "yes",
                "bulletControls"          : "yes",
                "thumbnails"              : "no",
                "thumbnailWidth"          : 122,
                "thumbnailHeight"         : 53,
                "thumbnailSlideDirection" : "horizontal", // horizontal or vertical
                "hoverPause"              : "yes",
                "autoRotate"              : "yes",
                "fullScreenRotator"       : "no", // image dimensions work well at 1500x915
                "fullScreenBreakpoints"   : ["960", "768", "640", "480", "320"], // OPTIONS ARE ["960", "768", "640", "480", "320"]
                "fullScreenVideo"         : "yes",
                "transitionType"          : "fade", // fade or slide
                "transitionSpeed"         : transitionSpeed,
                "transitionDelay"         : 6,
                "onImageLoad"             : function() { },
                "allImagesLoaded"         : function() { },
                "onThumbnailLoad"         : function() { },
                "allThumbnailsLoaded"     : function() { },
                "onTransitionStart"       : function() { },
                "onTransitionEnd"         : function() {
                	$("#hp-slideshow .rs-gallery-container").attr("data-active-index", $("#hp-slideshow ul.rs-gallery-images-list li.active").index());
                },
                "watchVidClick"           : function() { },
                "closeVidClick"           : function() { },
                "allLoaded"               : function() {
                	$(".rs-gallery-desc", this).wrap('<div class="rs-gallery-desc-outer"></div>');
                    $(".rs-gallery-desc", this).children().not($(".rs-gallery-title")).not($(".rs-gallery-caption")).wrapAll('<div class="rs-gallery-btns"></div>');
                	$("#hp-slideshow .rs-gallery-container").attr("data-active-index", $("#hp-slideshow ul.rs-gallery-images-list li.active").index());
                }
            });
        }
    }
    
    function adjustCardAppTitles() {
    	if($(".hp").length && $("#hp-slideshow .region > div:first-child .ui-widget.app.share-card-headlines").length && csGetBreakPoint() != "320") {
            var rowNum = 1;
            
            // RESET TITLE HEIGHTS
            $("#hp-slideshow .region > div:first-child .ui-widget.app.share-card-headlines .ui-articles li h1.ui-article-title").removeAttr("style");
            
            // CALCULATE HOW MANY ROWS AND SET ROW CLASSES FOR NEXT STEP
            $("#hp-slideshow .region > div:first-child .ui-widget.app.share-card-headlines .ui-articles li").each(function() {
            	if($(this).prev("li").length) {
                	if($(this).position().top == $(this).prev("li").position().top) {
                    	$(this).attr("class", "row-" + rowNum);
                    } else {
                    	rowNum++;
                        $(this).attr("class", "row-" + rowNum);
                    }
                } else {
                	$(this).attr("class", "row-" + rowNum);
                }
            });
            
            // LOOP THROUGH EACH ROW AND SET ALL TITLES TO EQUAL HEIGHT
            for(var i = 1; i <= rowNum; i++) {
            	var titleHeight = 0;
                
                // FIND THE TALLEST TITLE
                $("#hp-slideshow .region > div:first-child .ui-widget.app.share-card-headlines .ui-articles li.row-" + i).each(function() {
                	if($("h1.ui-article-title", this).height() > titleHeight) {
                    	titleHeight = $("h1.ui-article-title", this).height();
                    }
                });
                
                // APPLY THE TITLE HEIGHT TO ALL TITLES
                $("#hp-slideshow .region > div:first-child .ui-widget.app.share-card-headlines .ui-articles li.row-" + i + " h1.ui-article-title").css("height", titleHeight);
            }
        }
    }
    
    function checkSlideshow() {
		if($(".hp").length && csGetBreakPoint() != "desktop") {
			if ($(window).scrollTop() <= $("#hp-slideshow").offset().top + $("#hp-slideshow").height()) {
				if(csSlideshowDescFixed) {
					$("#hp-slideshow .rs-gallery-desc-outer").removeAttr("style");
					
					csSlideshowDescFixed = false;
				}
			} else {
				if(!csSlideshowDescFixed) {
					$("#hp-slideshow .rs-gallery-desc-outer").css({
						"height": $("#hp-slideshow .rs-gallery-desc-outer").height(),
						"overflow": "hidden"
					});
					
					csSlideshowDescFixed = true;
				}
			}
		}
	}
	
	function footerAccordion() {
		if(!csFooterAccordionIsSet) {
			// SET ROLES AND ATTRIBUTES
            $(".gb-footer.top .gb.footer").each(function() {
            	$(".gb-footer-menu", this).attr("aria-hidden", "true");
                $(".gb-footer-menu-header", this).attr("role", "button");
            });
			
			// ACCORDION HEADER CLICK
			$(".gb-footer.top .gb-footer-menu-header").click(function(){
                $(this).toggleClass("open");
                $(this).next(".gb-footer-menu").slideToggle("300", "swing");
                $(this).next(".gb-footer-menu").toggleClass("open");
                var ariaState = ($(this).hasClass("open")) ? "false" : "true";
                $(this).next(".gb-footer-menu").attr("aria-hidden", ariaState);
			});
		
			csFooterAccordionIsSet = true;
		}
	}
	
	function footerAccordionReset() {
		if(csFooterAccordionIsSet) {
			// REMOVE ROLES AND ATTRIBUTES
            $(".gb-footer.top .gb.footer").each(function() {
            	$(".gb-footer-menu", this).removeAttr("aria-hidden").removeClass("open").removeAttr("style");
                $(".gb-footer-menu-header", this).removeAttr("role").removeClass("open");
            });
            
            $(".gb-footer.top .gb-footer-menu-header").unbind("click");
			
			csFooterAccordionIsSet = false;
		}
	}
    
    function pageListAccordion() {
		if(!csPageListAccordionIsSet) {
        	// SET ROLES AND ATTRIBUTES
            $(".sp.column.one .ui-widget.app.pagenavigation .ui-widget-detail > .page-navigation").attr("role", "menu");
        	$(".sp.column.one .ui-widget.app.pagenavigation .ui-widget-header").attr("role", "button");
            $(".sp.column.one .ui-widget.app.pagenavigation .ui-widget-detail > .page-navigation ul").attr("role", "menu");
            $(".sp.column.one .ui-widget.app.pagenavigation .ui-widget-detail > .page-navigation li").attr("role", "menuitem");
            
			// OPEN PAGE NAVIGATION
			$(".sp.column.one div.ui-widget.app").addClass("active");
            $(".sp.column.one div.ui-widget.app .ui-widget-detail").show();
			
			// SUBPAGE NAVIGATION HEADER CLICK
			$(".sp.column.one div.ui-widget-header").click(function(){
				$(this).parent().toggleClass("active");
                $(".sp.column.one div.ui-widget.app .ui-widget-detail").slideToggle("300", "swing");
                var ariaState = ($(this).parent().hasClass("active")) ? "false" : "true";
                $(".sp.column.one div.ui-widget.app .ui-widget-detail").attr("aria-hidden", ariaState);
			});
			
			csPageListAccordionIsSet = true;
		}
	}
	
	function pageListAccordionReset() {
		if(csPageListAccordionIsSet) {
			// REMOVE ROLES AND ATTRIBUTES
            $(".sp.column.one div.ui-widget.app").removeClass("active");
        	$(".sp.column.one .ui-widget.app.pagenavigation .ui-widget-header").removeAttr("role");
            $(".sp.column.one div.ui-widget-header").unbind("click");
            $(".sp.column.one div.ui-widget.app .ui-widget-detail").removeAttr("style");
			
			csPageListAccordionIsSet = false;
		}
	}
    
    function makeNavAppAccessible() {
    	$(".ui-widget.app.navigation").each(function() {
        	$(".ui-widget-detail > ul", this).attr("aria-hidden", "false");
        	$(".ui-widget-detail ul", this).attr("role", "menu");
            $(".ui-widget-detail ul li", this).attr("role", "menuitem");
            $(".ui-widget-detail .bullet.expandable, .ui-widget-detail .bullet.collapsible", this).attr("role", "button").attr("aria-label", "Open/Close Submenu").attr("tabindex", "0");
            $(".ui-widget-detail .bullet ~ ul", this).attr("aria-hidden", "true");
            $(".ui-widget-detail ul li.expandable, .ui-widget-detail ul li.collapsible", this).attr("aria-haspopup", "true");
        });
        
        $(".ui-widget.app.navigation").on("keydown", ".ui-widget-detail ul li a", function(e) {
            switch(e.keyCode) {
            	case csKeyCodes.up:
                	e.preventDefault();
                    
                    if($(this).parent().is(":first-child")) {
                    	$(this).closest("ul").find("> li:last-child > a").focus();
                    } else {
                    	$(this).parent().prev("li").find("> a").focus();
                    }
                break;
                
                case csKeyCodes.down:
                	e.preventDefault();
                    
                    if($(this).parent().is(":last-child")) {
                    	$(this).closest("ul").find("> li:first-child > a").focus();
                    } else {
                    	$(this).parent().next("li").find("> a").focus();
                    }
                break;
                
                case csKeyCodes.left:
                	e.preventDefault();
                
                	if($(this).prev(".bullet.expandable").length || $(this).prev(".bullet.collapsible").length) {
                    	$(this).prev(".bullet").focus();
                    } else {
                    	if($(this).parent().is(":first-child")) {
                            $(this).closest("ul").find("> li:last-child > a").focus();
                        } else {
                            $(this).parent().prev("li").find("> a").focus();
                        }
                    }
                break;
                
                case csKeyCodes.right:
                	e.preventDefault();
                    
                    if($(this).parent().is(":last-child")) {
                    	if($(this).closest("ul").find("> li:first-child").hasClass("expandable")) {
                        	$(this).closest("ul").find("> li:first-child > .expandable").focus();
                        } else if($(this).closest("ul").find("> li:first-child").hasClass("collapsible")) {
                        	$(this).closest("ul").find("> li:first-child > .collapsible").focus();
                        } else {
                    		$(this).closest("ul").find("> li:first-child > a").focus();
                        }
                    } else {
                    	if($(this).parent().next("li").hasClass("expandable")) {
                        	$(this).parent().next("li").find("> .expandable").focus();
                        } else if($(this).parent().next("li").hasClass("collapsible")) {
                        	$(this).parent().next("li").find("> .collapsible").focus();
                        } else {
                    		$(this).parent().next("li").find("> a").focus();
                        }
                    }
                break;
                
                case csKeyCodes.home:
                	e.preventDefault();
                    
                    $(this).closest("ul").find("> li:first-child > a").focus();
                break;
                
                case csKeyCodes.end:
                	e.preventDefault();
                    
                    $(this).closest("ul").find("> li:last-child > a").focus();
                break;
            }
        });
        
        $(".ui-widget.app.navigation").on("keydown", ".bullet.expandable, .bullet.collapsible", function(e) {
        	switch(e.keyCode) {
            	case csKeyCodes.up:
                case csKeyCodes.left:
                	e.preventDefault();
                
                	if($(this).parent().is(":first-child")) {
                    	$(this).closest("ul").find("> li:last-child > a").focus();
                    } else {
                    	$(this).parent().prev("li").find("> a").focus();
                    }
                break;
                
                case csKeyCodes.down:
                case csKeyCodes.right:
                	e.preventDefault();
                
                	$("~ a", this).focus();
                break;
                
                case csKeyCodes.space:
                case csKeyCodes.enter:
                	e.preventDefault();
                
                	$(this).trigger("click");
                    $("~ ul", this).attr("aria-hidden", ($("~ ul", this).attr("aria-hidden") == "true") ? "false" : "true");
                    $("~ ul > li:first-child > a", this).focus();
                break;
            }
        });
        
        $(".ui-widget.app.navigation").on("keydown", ".ui-widget-detail ul li.expandable > ul a, .ui-widget-detail ul li.collapsible > ul a", function(e) {
        	switch(e.keyCode) {
            	case csKeyCodes.esc:
                	e.preventDefault();
                
                	$(this).closest("ul").parent().find(".bullet").trigger("click").next("a").focus();
                    $(this).closest("ul").attr("aria-hidden", ($("~ ul", this).attr("aria-hidden") == "true") ? "false" : "true");
                break;
            }
        });
        
        $(".ui-widget.app.navigation").on("click", ".bullet.expandable, .bullet.collapsible", function(e) {
        	$("~ ul", this).attr("aria-hidden", ($("~ ul", this).attr("aria-hidden") == "true") ? "false" : "true");
        });
    }
    
    function channelBarEvents() {
        $(".sw-channel-item").unbind('hover');
        $(".sw-channel-item").hover(function(){
        	$(".sw-channel-item ul").stop(true, true);
        	var subList = $(this).children('ul');
        	if ($.trim(subList.html()) !== "") {
        		subList.slideDown(300, "swing");
        	}
        	$(this).addClass("hover");
        }, function(){
        	$(".sw-channel-dropdown").slideUp(300, "swing");
        	$(this).removeClass("hover");
        });
    }
    
    function solutionsDropdownEvents() {
    	// BUTTON CLICK EVENT
        $("#gb-solutions-toggle").click(function(e) {
        	e.preventDefault();
            
        	$("#gb-solutions-menu").addClass("open");
        	$("#gb-solution-links").slideDown(300, "swing").attr("aria-hidden", "false");
        });
        
        // BUTTON KEYDOWN EVENTS
        $("#gb-solutions-toggle").keydown(function(e) {
        	// CAPTURE KEY CODE
        	switch(e.keyCode) {
                // CONSUME SPACE AND ENTER KEYS
                case csKeyCodes.space:
                case csKeyCodes.enter:
                    e.preventDefault();
					
                    // TOGGLE THE MENU
                    $("#gb-solutions-menu").toggleClass("open");
                    $("#gb-solution-links").slideToggle(300, "swing");
                    
                    if($("#gb-solutions-menu").hasClass("open")) {
                    	// FOCUS THE FIRST LINK
                        $("#gb-solution-links .gb-solution-link:first-child > a").focus();
                        
                        // ADJUST ARIA ATTRIBUTES
                        $("#gb-solution-links").attr("aria-hidden", "false");
                    } else {
                    	// ADJUST ARIA ATTRIBUTES
                        $("#gb-solution-links").attr("aria-hidden", "true");
                    }
                break;
                
                // CONSUME DOWN ARROW KEY
                case csKeyCodes.down:
                	e.preventDefault();
            		
                    // SHOW THE MENU
                    $("#gb-solutions-menu").addClass("open");
                    $("#gb-solution-links").slideDown(300, "swing").attr("aria-hidden", "false");
                    
                    // FOCUS THE FIRST LINK
                    $("#gb-solution-links .gb-solution-link:first-child > a").focus();
                break;
                
                // CONSUME ESC KEY
                case csKeyCodes.esc:
                	e.preventDefault();
            		
                    // HIDE THE MENU
                    $("#gb-solutions-menu").removeClass("open");
                    $("#gb-solution-links").slideUp(300, "swing").attr("aria-hidden", "true");
                break;
            }
        });
        
        // DROPDOWN MOUSELEAVE
        $("#gb-solutions-menu").mouseleave(function(e) {
        	e.preventDefault();
            
        	$("#gb-solutions-menu").removeClass("open");
            $("#gb-solution-links").slideUp(300, "swing").attr("aria-hidden", "true");
        });
        
        // DROPDOWN LINK KEYDOWN EVENTS
        $(".gb-solution-link > a").keydown(function(e) {
        	// CAPTURE KEY CODE
        	switch(e.keyCode) {
            	// CONSUME LEFT AND UP ARROWS
                case csKeyCodes.left:
                case csKeyCodes.up:
                	e.preventDefault();
                    
                    // IS FIRST ITEM
                    if($(this).parent().is(":first-child")) {
                        // FOCUS DROPDOWN BUTTON
                        $("#gb-solutions-toggle").focus();
                    } else {
                        // FOCUS PREVIOUS ITEM
                        $(this).parent().prev(".gb-solution-link").find("> a").focus();
                    }
                break;
                
                // CONSUME RIGHT AND DOWN ARROWS
                case csKeyCodes.right:
                case csKeyCodes.down:
                    e.preventDefault();

                    // IS LAST ITEM
                    if($(this).parent().is(":last-child")) {
                        // FOCUS FIRST ITEM
                        $(".gb-solution-link:first-child > a").focus();
                    } else {
                        // FOCUS NEXT ITEM
                        $(this).parent().next(".gb-solution-link").find("> a").focus();
                    }
                break;
                
                // CONSUME HOME KEY
                case csKeyCodes.home:
                	e.preventDefault();
                    
                    // FOCUS FIRST ITEM
                    $(".gb-solution-link:first-child > a").focus();
                break;
                
                // CONSUME END KEY
                case csKeyCodes.end:
                	e.preventDefault();
                    
                    // FOCUS LAST ITEM
                    $(".gb-solution-link:last-child > a").focus();
                break;
                
                // CONSUME ESC KEY
                case csKeyCodes.esc:
                	e.preventDefault();
            		
                    $("#gb-solutions-toggle").focus();
                    $("#gb-solutions-menu").removeClass("open");
                    $("#gb-solution-links").slideUp(300, "swing").attr("aria-hidden", "true");
                break;
            }
        });
    }
    
	function useChannelForSection(){
		// USE CHANNEL NAME FOR PAGELIST NAV HEADER IF ONE IS NOT PRESENT
		if(!$("div.sp.column.one .ui-widget-header h1").length) {
			$("div.sp.column.one .ui-widget-header").append("<h1></h1>");
		}
    }
        
	function dynamicContent(){
		// ********** MYSTART ********** //
		$(".sw-mystart-dropdown").unbind();
		$(".sw-mystart-dropdown").click(function(e) {
			$(".sw-dropdown", this).slideDown(300, "swing");
			$(this).addClass("open");
		});
		$(".sw-mystart-dropdown").hover(function(e) {
			e.preventDefault();
		}, function() {
            $(".sw-dropdown",this).slideUp(300, "swing");
			$(this).removeClass("open");
		});

		// ********** HEADER ********** //
        
        	$("#gb-mystart-links").append($(".sw-mystart-button.signin").html());
            if($(".sw-mystart-button.register").length) {
            	//$("#gb-mystart-links").append(" | " + $(".sw-mystart-button.register").html());
				$("#gb-mystart-links").append(" | " + $("#navc-39").html());
            }
        
		
		// ********** CHANNEL BAR ********** //
        // GET RID OF EMPTY CHANNEL DROPDOWNS
        $("#sw-channel-list-container .sw-channel-dropdown").each(function() {
        	if(!$("li", this).length) $(this).remove();
        });
		
		// ********** BODY ********** //
		// AUTO FOCUS SIGN IN FIELD
		$("#swsignin-txt-username").focus();
        // CENTER SIGNED OUT MESSAGE AND SIGNIN BUTTON
        if($("div.ui-spn > div > p > span").text() == "You have been signed out."){
        	$("div.ui-spn > div").css({"text-align" : "center", "padding" : "50px 0px 50px 0px"});
        	//DOC add signed out breadcrumb
        	$(".ui-breadcrumbs").append("<li class='ui-breadcrumb-last'>Signed Out</li>");
        }
        // ADJUST SIGN IN PAGE
        if($("#swlogin").length){
        	$("#sw-content-layout-wrapper .ui-widget.app").addClass("signin");
        	$(".ui-breadcrumbs").append("<li class='ui-breadcrumb-last'>Sign In</li>");
        }
        // ADJUST REGISTER PAGE
		if($("#swageprompt-txt-birthmonth").length && $("#swageprompt-txt-birthday").length && $("#swageprompt-txt-birthyear").length) {
			$("#sw-content-layout-wrapper .ui-widget.app").addClass("register");
			$(".ui-breadcrumbs").append("<li class='ui-breadcrumb-last'>Register</li>");
		}
		// ADJUST SEARCH RESULTS PAGE
		if($("#sw-content-layout-wrapper.ui-spn #swsearch-pnl-main").length) {
			$("#sw-content-layout-wrapper").children().wrapAll('<div class="ui-widget app search-results"><div class="ui-widget-detail"></div></div>');
			$(".ui-breadcrumbs").append("<li class='ui-breadcrumb-last'>Search Results</li>");
		}
        // APPLY RESPONSIVE DIMENSIONS TO CONTENT IMAGES
        $("div.ui-widget.app .ui-widget-detail img")
            .not($("div.ui-widget.app.cs-rs-multimedia-rotator .ui-widget-detail img"))
            .not($("div.ui-widget.app.gallery.json .ui-widget-detail img"))
            .each(function() {
                if ($(this).attr('width') !== undefined && $(this).attr('height') !== undefined) { // IMAGE HAS INLINE DIMENSIONS
                    $(this).css({"display": "inline-block", "width": "100%", "max-width": $(this).attr("width") + "px", "height": "auto", "max-height": $(this).attr("height") + "px"});
                }
        });
        // REVERSE EVENT TIME AND TITLE
        $(".ui-widget.app.upcomingevents .ui-article-description").each(function() {
        	if(!$(".sw-calendar-block-time", this).length && $(this).text() != "There are no upcoming events to display.") {
            	$(this).append('<span class="sw-calendar-block-time all-day">All DAY</span>');
            } else {
            	$(this).prepend($(".sw-calendar-block-title", this));
            }
        });
        $(".ui-widget.app.image img").css({
        	"width": "",
            "height": ""
        });
        	
		
		// ********** FOOTER ********** //
        // GRAB THE SITE LIST
        $(".gb.footer.three .gb-footer-menu.three").append('<li><a href="/">Consulting Catalog</a></li>' + $(".sw-mystart-dropdown.schoollist .sw-dropdown-list").html());
        var linkClasses = ["one", "two", "three", "four", "five", "six", "seven", "eight", "nine", "ten", "eleven", "twelve", "thirteen", "fourteen", "fifteen", "sixteen", "seventeen", "eighteen", "nineteen", "twenty"];
        
        $(".gb.footer.three .gb-footer-menu.three li").each(function(i) {
        	$(this).addClass("gb-footer-menu-link " + linkClasses[i]).attr("role", "menuitem");
            $("a", this).removeAttr("tabindex");
        });
        
        // REMOVE UNUSED FOOTER LINKS
        $(".gb-solution-link.false, .gb-footer-menu-link.false, .gb-footer-social-icon.false").remove();
        
        // FOOTER MAP LINK
        $("#gb-footer-map-link").attr("href", "//www.google.com/maps/place/" + encodeURIComponent("1111 19th Street N.W. Washington DC 20036"));
        
        // SCHOOLWIRES LINKS
		$("#gb-footer-terms").attr("href", $("#sw-footer-links li:eq(2) a").attr("href"));
        $("#gb-footer-policy").attr("href", $("#sw-footer-links li:eq(1) a").attr("href"));
        
        // SCROLL TO TOP BUTTON
        $("#gb-scroll-to-top").click(function(e){
			e.preventDefault();
			$("html, body").animate({ scrollTop: 0 }, "slow");
		});
	}
    
    function globalIcons() {
    	$("#gb-global-icons").creativeIcons({
            "iconNum"       : "6", // NUMBER OF ICONS
            "icons"         : [
                    ["/cms/lib/BBCS000002/Centricity/Template/GlobalAssets/icons/Consulting-01.png", "Student Success: ", "/Page/57", "_self"], // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
                    ["/cms/lib/BBCS000002/Centricity/Template/GlobalAssets/icons/Consulting-02.png", "Student Retention: ", "/Page/58", "_self"], // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
                    ["/cms/lib/BBCS000002/Centricity/Template/GlobalAssets/icons/Consulting-03.png", "Accessibility: ", "/Page/59", "_self"], // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
                    ["/cms/lib/BBCS000002/Centricity/Template/GlobalAssets/icons/Consulting-04.png", "Educational Integrity: ", "/Page/60", "_self"], // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
                    ["/cms/lib/BBCS000002/Centricity/Template/GlobalAssets/icons/Consulting-05.png", "Educator Empowerment: ", "/Page/61", "_self"], // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
                    ["/cms/lib/BBCS000002/Centricity/Template/GlobalAssets/icons/Consulting-06.png", "Institutional Effectiveness: ", "/Page/66", "_self"] // ARRAY ORDER - IMAGE, TEXT, LINK, TARGET
            ],
            "siteID"        : "4",
            "siteAlias"     : "consultingcatalog",
            "calendarLink"  : "/Page/2",
            "contactEmail"  : "ConsultingCatalogPortal@blackboard.com",
            "allLoaded"     : function(){
            	$("#gb-global-icons .global-icon").each(function() {
                	$(".icon img", this).attr("width", "60").attr("height", "60");
                    
                    var iconText = $(".text", this).text().split(":");
                    $(".text", this).html('<div class="text-one">' + $.trim(iconText[0]) + '</div><div class="text-two">' + $.trim(iconText[1]) + '</div>');
                });
            }
        });
    }
    
    function bioAppBuildView() {
    	// BUILD THE THREE-COLUMN VIEW
        var i = 1;
        $(".ui-widget.app.sw-share-bio .ui-articles li").each(function() {
        	$(".sw-share-bio-columns .sw-share-bio-column.column-" + i).append('<div class="sw-share-bio-item">' + $(this).clone().html() + '</div>');
            
            i++;
            if(i == 4) i = 1;
        });
        
        // MARK THE DESCRIPTIONS THAT ARE TRUNCATED 
        $(".sw-share-bio-columns .sw-share-bio-item").each(function() {
        	// IF THERE THE DESCRIPTION IS TALLER THAN THE FIXED HEIGHT CONTAINER
            if($(".item-full-description > p", this).height() > $(".item-full-description", this).height()) {
            	$(".item-full-description > p", this).wrapInner('<span class="item-full-description-inner"></span>');
                $(".item-full-description > p", this).append('<span class="item-full-description-truncated">' + $(".item-full-description > p .item-full-description-inner", this).html() + '</span>');
                $(".item-full-description-truncated", this).ellipsis();
            }
        });
        
        // ITEM HOVER
        $(".sw-share-bio-item").hover(function() {
        	$(".item-full-description", this).stop(true, true);
            
            // IF THERE THE DESCRIPTION IS TALLER THAN THE FIXED HEIGHT CONTAINER
            if($(".item-full-description > p", this).height() > $(".item-full-description", this).height()) {
                $(".item-full-description", this).animate({
                    height: $(".item-full-description > p", this).height()
                }, 300);
            }
        }, function() {
        	$(".item-full-description", this).animate({
                height: "192px"
            }, 300);
        });
    }
    
    function scrollBtn() {
    	if(!csScrollBtnReset && csGetBreakPoint() == "desktop") {
        	$("#gb-scroll-to-top-container-outer").removeAttr("style");
            csScrollBtnReset = true;
            csScrollBtnIsSticky = false;
            csScrollBtnIsVisible = false;
        } else {
        	if($(".hp").length) {
                if ($(window).scrollTop() < $("#hp-slideshow").offset().top + $("#hp-slideshow").height()) {
                    if(csScrollBtnIsSticky) {
                        $("#gb-scroll-to-top-container-outer").fadeOut(300, function() {
                            $(this).removeClass("sticky").removeAttr("style");
                        });

                        csScrollBtnIsSticky = false;
                        csScrollBtnReset = false;
                        csScrollBtnIsVisible = false;
                    }
                } else if($(window).scrollTop() >= ($("#hp-slideshow").offset().top + $("#hp-slideshow").height()) && $(window).scrollTop() < (($(".gb-footer-outer.top").offset().top + 50.5) - $(window).height())) {
                    if(!csScrollBtnIsSticky && csScrollBtnIsVisible || !csScrollBtnIsSticky && !csScrollBtnIsVisible) {
                        $("#gb-scroll-to-top-container-outer").addClass("sticky").fadeIn();

                        csScrollBtnIsSticky = true;
                        csScrollBtnReset = false;
                        csScrollBtnIsVisible = true;
                    }
                } else if($(window).scrollTop() >= (($(".gb-footer-outer.top").offset().top + 50.5) - $(window).height())) {
                    if(csScrollBtnIsSticky || !csScrollBtnIsVisible) {
                        $("#gb-scroll-to-top-container-outer").removeClass("sticky").fadeIn();

                        csScrollBtnIsSticky = false;
                        csScrollBtnReset = false;
                        csScrollBtnIsVisible = true;
                    }
                }
            } else {
            	if ($(window).scrollTop() < $("#sw-content-layout-wrapper").offset().top) {
                    if(csScrollBtnIsSticky) {
                        $("#gb-scroll-to-top-container-outer").fadeOut(300, function() {
                            $(this).removeClass("sticky").removeAttr("style");
                        });

                        csScrollBtnIsSticky = false;
                        csScrollBtnReset = false;
                        csScrollBtnIsVisible = false;
                    }
                } else if($(window).scrollTop() >= ($("#sw-content-layout-wrapper").offset().top) && $(window).scrollTop() < (($(".gb-footer-outer.top").offset().top + 50.5) - $(window).height())) {
                    if(!csScrollBtnIsSticky && csScrollBtnIsVisible || !csScrollBtnIsSticky && !csScrollBtnIsVisible) {
                        $("#gb-scroll-to-top-container-outer").addClass("sticky").fadeIn();

                        csScrollBtnIsSticky = true;
                        csScrollBtnReset = false;
                        csScrollBtnIsVisible = true;
                    }
                } else if($(window).scrollTop() >= (($(".gb-footer-outer.top").offset().top + 50.5) - $(window).height())) {
                    if(csScrollBtnIsSticky || !csScrollBtnIsVisible) {
                        $("#gb-scroll-to-top-container-outer").removeClass("sticky").fadeIn();

                        csScrollBtnIsSticky = false;
                        csScrollBtnReset = false;
                        csScrollBtnIsVisible = true;
                    }
                }
            }
        }
    }
	
	function searchText() {
    	$(document).click(function() {
        	if($("#gb-search").hasClass("open")) {
            	$("#gb-search").removeClass("open");
                $("#gb-universal-navigation").removeClass("search-open");
            }
        });
    	$("#gb-search").click(function(e) {
        	 e.preventDefault();
            e.stopPropagation();
            $(this).addClass("open");
            $("#gb-universal-navigation").addClass("search-open");
            $("#gb-search-input").focus();
        });
		$("#gb-search-button").click(function(e){
            e.preventDefault();
            e.stopPropagation();
            if($("#gb-search-input").val() != "Search..." && $.trim($("#gb-search-input").val()) != "") {
                var searchForm = $(this).parent();
                window.location.href = "/site/Default.aspx?PageType=6&SiteID=4&SearchString=" + $(".gb-search-input", searchForm).val();
            } else {
            	$("#gb-search").addClass("open");
                $("#gb-universal-navigation").addClass("search-open");

                $("#gb-search-input").val("").focus();
            }
        });
        $("#gb-search-form").submit(function(e){
            e.preventDefault();
            e.stopPropagation();
            var searchForm = $(this).parent();
            window.location.href = "/site/Default.aspx?PageType=6&SiteID=4&SearchString=" + $(".gb-search-input", searchForm).val();
        });
        $("#gb-search-input").focus(function() {
            $("#gb-search").addClass("open");
            $("#gb-universal-navigation").addClass("search-open");
            
            if($(this).val() == "Search...") {
                $(this).val("");
            }
        });
        $("#gb-search-input").blur(function() {
        	if(csGetBreakPoint() == "desktop") {
                if($("#gb-search").hasClass("open")) {
                    $("#gb-search").removeClass("open");
                    $("#gb-universal-navigation").removeClass("search-open");
                }
            }
            if(csGetBreakPoint() == "desktop" && $(this).val() == "") {
                $(this).val("Search...");
            }
        });
	}
	
	function calendarListView(){
		// AUTO CLICK FUNCTION FOR SCHOOLWIRES CALENDAR LIST VIEW FOR SMALL MOBILE DEVICES
		if(!$(".ui-widget.app.calendar #calendarlist-pnl-specialview.loaded").length){
			$("a.ui-btn-toggle.list").click();
			$("a.ui-btn-toggle.month").click();
			$(".ui-widget.app.calendar #calendarlist-pnl-specialview").addClass("loaded");
		}
		
		switch(csGetBreakPoint()) {
			case "desktop": case "768": case "640":
				$(".ui-widget.app.calendar #calendar").removeClass("ui-helper-hidden");
				$(".ui-widget.app.calendar #calendarlist-pnl-specialview").addClass("ui-helper-hidden");
			break;
			case "480": case "320":
				$(".ui-widget.app.calendar #calendar").addClass("ui-helper-hidden");
				$(".ui-widget.app.calendar #calendarlist-pnl-specialview").removeClass("ui-helper-hidden");
				$(".ui-btn-toggle.month").removeClass("active");
				$(".ui-btn-toggle.list").addClass("active");
			break;
		}
	}
	
	function csGetBreakPoint() {
		return window.getComputedStyle(document.querySelector("body"), ":before").getPropertyValue("content").replace(/"|'/g, "");/*"*/
	}
    
    /*function googleTranslateElementInitVertical() {
        new google.translate.TranslateElement({pageLanguage: 'en', layout: google.translate.TranslateElement.InlineLayout.VERTICAL}, 'google_translate_element');
    	$("#google_translate_element .goog-te-combo").attr("id", "google-select-dropdown");
        $('<label for="google-select-dropdown">Select Language</label>').insertBefore("#google_translate_element .goog-te-combo");
        $(".goog-logo-link img, #goog-gt-tt .logo > img:first-of-type").attr("alt", "Google Translate");
    }*/
    
    function csTranslate() {
    	$(".cs-mystart-dropdown.translate .cs-dropdown").creativeTranslate({
            "type": 2, // 1 = FRAMESET, 2 = BRANDED, 3 = API
            "advancedOptions": {
                "addMethod": "append", // PREPEND OR APPEND THE TRANSLATE ELEMENT
                "dropdownHandleText": "Translate this page...", // ONLY FOR FRAMESET AND API VERSIONS AND NOT USING A CUSTOM ELEMENT
                "customElement": { // ONLY FOR FRAMESET AND API VERSIONS
                    "useCustomElement": false, 
                    "translateItemsList": true, // true = THE TRANSLATE ITEMS WILL BE AN UNORDERED LIST, false = THE TRANSLATE ITEMS WILL JUST BE A COLLECTION OF <a> TAGS
                    "customElementMarkup": "" // CUSTOM HTML MARKUP THAT MAKES THE CUSTOM TRANSLATE ELEMENT/STRUCTURE - USE [$CreativeTranslateListItems$] ACTIVE BLOCK IN THE MARKUP WHERE THE TRANSLATE ITEMS SHOULD BE ADDED
                },
                "apiKey": "", // ONLY FOR API VERSION
                "brandedLayout": 1 // 1 = VERTICAL (TEXT UNDER SELECT), 2 = HORIZONTAL (TEXT BESIDE SELECT), 3 = NON-RESPONSIVE MEGA DROPDOWN
            },
            "translateLoaded": function() {}
        });

        // BIND DROPDOWN EVENTS
        dropdownActions({
            "dropdownParent": ".cs-mystart-dropdown.translate",
            "dropdownSelector": ".cs-selector",
            "dropdown": ".cs-dropdown",
            "dropdownList": ".cs-dropdown-list"
        });
        
        // APPLY TRANSLATE URLS
        var brandedPageUrl = (window.location.href).replace(/#googtrans\(en\|[\s\S].*?\)/gi, "");
        $(".cs-mystart-dropdown.translate .cs-dropdown-list a.cs-branded-link").each(function() {
        	$(this).attr("href", brandedPageUrl + "#googtrans(" + $(this).attr("data-lang-code") + ")");
        });
        
        // BRANDED TRANSLATE CLICK
        $(".cs-mystart-dropdown.translate .cs-dropdown-list a.cs-branded-link").click(function(e) {
            location.reload();
        }).keydown(function(e) {
            if(e.keyCode == 13) {
                location.reload();
            }
        });
    }
    
    function dropdownActions(params) {
        	// FOR SCOPE
            var template = this;
            
			var KeyCodes = { "tab": 9, "enter": 13, "esc": 27, "space": 32, "end": 35, "home": 36, "left": 37, "up": 38, "right": 39, "down": 40 };
            var dropdownParent = params.dropdownParent;
            var dropdownSelector = params.dropdownSelector;
            var dropdown = params.dropdown;
            var dropdownList = params.dropdownList;
            
            $(dropdownParent + " " + dropdownList + " a").attr("tabindex", "-1");
            
            // MYSTART DROPDOWN SELECTOR CLICK EVENT
            $(dropdownParent).on("click", dropdownSelector, function(e) {
                e.preventDefault();
                
                if($(this).parent().hasClass("open")){
                	$("+ " + dropdown + " " + dropdownList + " a").attr("tabindex", "-1");
                    $(this).attr("aria-expanded", "false").parent().removeClass("open").find(dropdown).attr("aria-hidden", "true").slideUp(300, "swing"); 
                } else {
                	$(this).attr("aria-expanded", "true").parent().addClass("open").find(dropdown).attr("aria-hidden","false").slideDown(300, "swing");
                }
        	});
            
            // MYSTART DROPDOWN SELECTOR KEYDOWN EVENTS
            $(dropdownParent).on("keydown", dropdownSelector, function(e) {
                // CAPTURE KEY CODE
                switch(e.keyCode) {
                    // CONSUME LEFT AND UP ARROWS
                    case KeyCodes.enter:
                    case KeyCodes.space:
                        e.preventDefault();

                        // IF THE DROPDOWN IS OPEN, CLOSE IT
                        if($(dropdownParent).hasClass("open")){
                        	$("+ " + dropdown + " " + dropdownList + " a").attr("tabindex", "-1");
                            $(this).attr("aria-expanded", "false").parent().removeClass("open").find(dropdown).attr("aria-hidden", "true").slideUp(300, "swing"); 
                        } else {
                        	$(this).attr("aria-expanded", "true").parent().addClass("open").find(dropdown).attr("aria-hidden", "false").slideDown(300, "swing", function(){
                                $(dropdownList + " li:first-child a", this).attr("tabindex", "0").focus();
                            });
                        }
                    break;

                    // CONSUME TAB KEY
                    case KeyCodes.tab:
                    	if($("+ " + dropdown + " " + dropdownList + " a").length) {
                        	$("+ " + dropdown + " " + dropdownList + " a").attr("tabindex", "-1");
                        	$(this).attr("aria-expanded", "false").parent().removeClass("open").find(dropdown).attr("aria-hidden", "true").slideUp(300, "swing");
                        }
                    break;

                    // CONSUME LEFT AND UP ARROWS
                    case KeyCodes.down:
                    case KeyCodes.right:
                        e.preventDefault();
						
                        $("+ " + dropdown + " " + dropdownList + " a").attr("tabindex", "-1");
                        $("+ " + dropdown + " " + dropdownList + " li:first-child > a", this).attr("tabindex", "0").focus();
                    break;
                }
            });
			
            // MYSTART DROPDOWN LINK KEYDOWN EVENTS
            $(dropdownParent).on("keydown", dropdownList + " li a", function(e) {
                // CAPTURE KEY CODE
                switch(e.keyCode) {
                    // CONSUME LEFT AND UP ARROWS
                    case KeyCodes.left:
                    case KeyCodes.up:
                        e.preventDefault();

                        // IS FIRST ITEM
                        if($(this).parent().is(":first-child")) {
                            // FOCUS DROPDOWN BUTTON
                            $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                            $(this).closest(dropdownParent).find(dropdownSelector).focus();
                        } else {
                            // FOCUS PREVIOUS ITEM
                            $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                            $(this).parent().prev("li").find("> a").attr("tabindex", "0").focus();
                        }
                    break;

                    // CONSUME RIGHT AND DOWN ARROWS
                    case KeyCodes.right:
                    case KeyCodes.down:
                        e.preventDefault();

                        // IS LAST ITEM
                        if($(this).parent().is(":last-child")) {
                            // FOCUS FIRST ITEM
                            $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                            $(this).closest(dropdownList).find("li:first-child > a").attr("tabindex", "0").focus();
                        } else {
                            // FOCUS NEXT ITEM
                            $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                            $(this).parent().next("li").find("> a").attr("tabindex", "0").focus();
                        }
                    break;
                    
                    // CONSUME TAB KEY
                    case KeyCodes.tab:
                    	if(e.shiftKey) {
                        	e.preventDefault();
                            
                        	// FOCUS DROPDOWN BUTTON
                            $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                            $(this).closest(dropdownParent).find(dropdownSelector).focus();
                        }
                    break;

                    // CONSUME HOME KEY
                    case KeyCodes.home:
                        e.preventDefault();

                        // FOCUS FIRST ITEM
                        $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                        $(this).closest(dropdownList).find("li:first-child > a").attr("tabindex", "0").focus();
                    break;

                    // CONSUME END KEY
                    case KeyCodes.end:
                        e.preventDefault();

                        // FOCUS LAST ITEM
                        $(this).closest(dropdownList).find("a").attr("tabindex", "-1");
                        $(this).closest(dropdownList).find("li:last-child > a").attr("tabindex", "0").focus();
                    break;
                }
            });
            
            var dropdownTimer;
            $(dropdownParent).mouseenter(function() {
            	clearTimeout(dropdownTimer);
            }).mouseleave(function() {
            	var thisDropdownParent = this;
                
				dropdownTimer = setTimeout(function(){
                	$(dropdownList + " a", thisDropdownParent).attr("tabindex", "-1");
					$(dropdownSelector, thisDropdownParent).attr("aria-expanded", "false").parent().removeClass("open").find(dropdown).attr("aria-hidden", "true").slideUp(300, "swing"); 
            	}, 500);
            }).focusout(function() {
                var thisDropdownParent = this;
                
                setTimeout(function () {
                    if(!$(thisDropdownParent).find(":focus").length) {
                        $(dropdownSelector, thisDropdownParent).attr("aria-expanded", "false").parent().removeClass("open").find(dropdown).attr("aria-hidden", "true").slideUp(300, "swing");
                    }
                }, 500);
            });
        }
</script>

    <!-- App Preview -->
    


    <style type="text/css">
        /* HOMEPAGE EDIT THUMBNAIL STYLES */

        div.region {
            ;
        }

            div.region span.homepage-thumb-region-number {
                font: bold 100px verdana;
                color: #fff;
            }

        div.homepage-thumb-region {
            background: #264867; /*dark blue*/
