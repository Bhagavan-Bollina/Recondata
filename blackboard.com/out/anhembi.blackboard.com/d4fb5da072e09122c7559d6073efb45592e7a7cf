  <script type="text/javascript">
            function autoLogin() {}
        </script>
    <script type="text/javascript">
    function loadLoginPage()
    {
        if (top != self)
        {
            top.location.replace(self.location.href);
        }
        if (document.forms.login.user_id != undefined)
        {
            document.forms.login.user_id.focus();
        }

        console.log(document.forms.namedItem("login").elements["entry-login"].value);
        document.forms.namedItem("login").elements["entry-login"].onclick = adicionaPrefixo;
        console.log(document.forms.namedItem("login").elements["entry-login"].onclick);
        setTimeout("triggerScreenreaderAlert()", 500);
    }



    function triggerScreenreaderAlert()
    {
        if (document.getElementById('loginErrorMessage'))
        {
            $('loginErrorMessage').update($('loginErrorMessage').innerHTML);
            console.log('alterou error')
            document.forms.namedItem("login").elements["user_id"].value = '';
            console.log('limpou')
        }
    }

    function adicionaPrefixo() {

        var prefixo;
        switch(window.location.hostname) {
           case "anhembi.blackboard.com":
           prefixo = "01";
           break;
           case "bsp.blackboard.com":
           prefixo = "02";
           break;
           case "unifacs.blackboard.com":
           prefixo = "03";
           break;
           case "unp.blackboard.com":
           prefixo = "04";
           break;
           case "uniritter.blackboard.com":
           prefixo = "05";
           break;
           case "fmu.blackboard.com":
           prefixo = "06";
           break;
           case "uninorte.blackboard.com":
           prefixo = "07";
           break;
           case "ibmr.blackboard.com":
           prefixo = "08";
           break;
           case "fg.blackboard.com":
           prefixo = "09";
           break;
           case "fpb.blackboard.com":
           prefixo = "10";
           break;
           case "fadergsead.blackboard.com":
           case "fadergs.blackboard.com":
           prefixo = "11";
           break;
           case "cedepe.blackboard.com":
           prefixo = "01";
           break;
           case "fapa.blackboard.com":
           prefixo = "13";
           break;
           case "fiam-faam.blackboard.com":
           prefixo = "14";
           break;
           default:
           prefixo = "";
       }

       if (prefixo != "" && document.forms.namedItem("login").elements["user_id"].value.trim() != "" 
        && document.forms.namedItem("login").elements["user_id"].value.trim().substring(0,3) != prefixo + "."){
           document.forms.namedItem("login").elements["user_id"].value = prefixo + "." + document.forms.namedItem("login").elements["user_id"].value.trim();
   } else {
       document.forms.namedItem("login").elements["user_id"].value = document.forms.namedItem("login").elements["user_id"].value.trim();
   }
}


</script>

<script>
    var ies = window.location.hostname;

    function estilo() {

       if(document.getElementById("lost-password"))    var lostPassword = document.getElementById("lost-password");
       if (ies == "anhembi.blackboard.com"){
        document.getElementById("css").setAttribute('href', 'https://laureatebrasil.blackboard.com/branding/themes/LAUREATE_PN/images/uam.css');
        window.location.assign("https://servicosuam.eadlaureate.com.br/blackboard/");
    }   else if ( ies == "bsp.blackboard.com") {
     document.getElementById("css").setAttribute('href', 'https://laureatebrasil.blackboard.com/branding/themes/LAUREATE_PN/images/bsp.css');
     lostPassword.href = 'http://satelites.anhembi.br/php/processo_seletivo/candidato/enviarsenha/';
 } else if ( ies == "uniritter.blackboard.com") {
     document.getElementById("css").setAttribute('href', 'https://laureatebrasil.blackboard.com/branding/themes/LAUREATE_PN/images/uniritter.css');
     window.location.assign("https://servicos.uniritter.edu.br/blackboard/");
 } else if ( ies == "unp.blackboard.com") {
     document.getElementById("css").setAttribute('href', 'https://laureatebrasil.blackboard.com/branding/themes/LAUREATE_PN/images/unp.css');
