    <script type="text/javascript">
      document.title = 'Ambiente Virtual do Senac';
      var isTouchDevice = navigator.userAgent.match(/(iPhone|iPod|iPad|Android|BlackBerry|Windows Phone)/);

      var sURL = window.location.origin; 
      var arrayURL = sURL.split("//");
      var urlFrom = arrayURL[1].split(".");
      namePage = urlFrom[0];

      var url = new URL(window.location); 
      var nonce = url.searchParams.get("nonce"); 

      if(nonce === '0') {
        nonce = document.getElementsByName('blackboard.platform.security.NonceUtil.nonce')[0].value;
        document.location.replace('https://www.sp.senac.br/login-unico/LoginMobileBlackBoard?frames=mobile&nonce='+nonce);
      }

      $(document).ready(function(e) {
        setTimeout(function() {
            $('#user_id').addClass('input-text').attr('placeholder','Usuário').parent().addClass('content-input');

            var prefixo = 'BRA';
            
            //ALTERADO TESTE window.location.hostname
            var host = window.location.hostname;
            
            if (host.indexOf('senac.blackboard.com') !== -1 || host.indexOf('tec') !== -1) {
              $clone = $('#user_id').clone();
              $clone.attr('id', 'user_tmp').attr('name', 'user_tmp').addClass('temp');
              $clone.prependTo($('.content-input').first());
              $clone.bind('focusout', function(){ $('#user_id').val(prefixo + $(this).val()); });
              
              $('#user_id').attr('tabindex','999').on('focus', function(){ $clone.focus(); });
            }
            
            $('#password').addClass('input-text').attr('placeholder','Senha').parent().addClass('content-input').attr('tabindex','2');
            $('#entry-login').removeAttr('class').val('Entrar').addClass('form-submit');
            $('input').placeholder();
            
            $("#loginFormList").find('li').last().addClass('content-links');

            $totalLi = $("#loginFormList").find('li').length - 1;
            $("#loginFormList").find('li').eq($totalLi).after($("#loginFormList").find('li').eq($totalLi-1));

            $('.content-links').append('<a class="form-link chamaPopup" href="javascript:void(0)" rel="1">Pré-requisitos técnicos </a>');
            $('.content-links').find('.forgot').children('a').addClass('form-link');

            /* VERIFICA SE A PLATAFORMA É MOBILE */
            if (isTouchDevice == null) {
            // LOAD FILES
            function loadjscssfile(filename, filetype){
              if (filetype=="js"){ //if filename is a external JavaScript file
                var fileref=document.createElement('script')
                fileref.setAttribute("type","text/javascript")
                fileref.setAttribute("src", filename)
              }
              else if (filetype=="css"){ //if filename is an external CSS file
                var fileref=document.createElement("link")
                fileref.setAttribute("rel", "stylesheet")
                fileref.setAttribute("type", "text/css")
                fileref.setAttribute("href", filename)
              }
              if (typeof fileref!="undefined")
                document.getElementsByTagName("head")[0].appendChild(fileref)
            }
          } else {
            $('#bb_checker').css('display', 'none'); }
          
            /* POPUP COM REQUISITOS */
            var altura;
            var largura;
            var popup_visivel = false;
            var addFile = false;

            /* REDIMENSIONA A TELA */
            function redimensiona() {
              largura =  ($(window).width() - $('#popup').width()) / 2;
              altura = ($(window).height() - $('#popup').height())  / 2;
            }
            
            /* ABRE POPUP */
            $(".chamaPopup").click(function(){
              if (isTouchDevice == null) {
                if (!addFile) {
                  addFile = true;
                loadjscssfile("https://www.java.com/js/deployJava.js", "js");
                loadjscssfile("/bbcswebdav/institution/login/scripts/bb.browserChecker.js", "js"); }}
                
              popup_visivel = true;
              redimensiona();
              $('#overlay_pop').css('filter', 'alpha(opacity=50)');
              $('#overlay_pop').fadeIn('fast',function(){
                $('#popup').css({ display:'block', position: 'absolute', left: largura });
                $('#popup').animate({'top': altura+'px'},500);
              });
            });
            /* FECHA POPUP */
            $('.close').click(function(){	
                popup_visivel = false;	
                var popCenter =  $('#popup').height();
                $('#popup').animate({'top': '-' + popCenter + 'px'},500,function(){
                $('#popup').css({display:'none'});
                $('#overlay_pop').fadeOut('fast');
              });
            });
            $('#overlay_pop').click(function() { $('.close').trigger('click'); });

            /* FECHA POPUP COM TECLADO */
            $(document).keyup(function(event){
                if (event.keyCode == 27) {
                  popup_visivel = false;  
                  var popCenter =  $('#popup').height();
                  $('#popup').animate({'top': '-' + popCenter + 'px'},500,function(){
                    $('#popup').css({display:'none'});
                    $('#overlay_pop').fadeOut('fast');
                  });
                }
            });
        
            /* QUANDO REDIMENSIONAR A TELA, AJUSTA O POPUP */		
            $(window).resize(function() {
              if(popup_visivel == true) {
                redimensiona();
                $('#popup').css({'left': largura+'px'});
                $('#popup').css({'top': altura+'px'});
              }
            });

            // ALTERADO TESTE
            var str = window.location.hostname;
            if (str.indexOf('corp') !== -1 || str.indexOf('senac.blackboard.com') !== -1 || str.indexOf('tec') !== -1 ) {
              $('#logoff').hide();
            } else { 
              $('#login').hide();
              $('#logoff').show();
            }
        }, 100);
      });

      function loadLoginPage()
      {
    	  if ( top != self )
    	  {
    		  top.location.replace( self.location.href );
    		}
    	  if(document.forms.login.user_id != undefined)
    		{
    		  document.forms.login.user_id.focus();
    		}
    	  setTimeout("triggerScreenreaderAlert()", 500);
      }
      
      function triggerScreenreaderAlert()
      {
    	if ( document.getElementById( 'loginErrorMessage' ) )
  	    {
    	  $( 'loginErrorMessage' ).update( $('loginErrorMessage').innerHTML );
  	    }
      }
    </script>
  
 
